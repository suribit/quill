.ql-video-tooltip
  padding: 10px
  width: 300px
  &:after
    clear: both
    content: ""
    display: table
  a
    border: 1px solid black
    box-sizing: border-box
    display: inline-block
    float: left
    padding: 5px
    text-align: center
    width: 50%
  .input
    box-sizing: border-box
    width: 100%
  
